generator client {
  provider = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Term {
  id          String   @id
  title       String
  description String
  source      String
  sourceUrl   String
  related     String[]
  position    Int
}
